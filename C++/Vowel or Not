class Solution{   
public:
    string isVowel(char c){
        if  (c=='a' ||c=='A' 
            || c=='e' || c=='E' 
            || c=='i' || c=='I' 
            || c=='o' || c=='O' 
            || c=='u' || c=='U')
        {
            return "YES";
        }
        else{
            return "NO";
        }
}
};
