class Solution
{
	public:
	bool ispar(string s)
	{
		stack<char>S;
		for(int i=0 ; i<s.size() ; i++)
		{
			if(s[i]=='(' || s[i]=='{' || s[i]=='[')
			S.push(s[i]);
			else
			{ 
				if(S.empty())   
				return false;
                
				if(s[i]==')' && S.top()!='(')   
				return false;
                
				if(s[i]=='}' && S.top()!='{')   
				return false;
                
				if(s[i]==']' && S.top()!='[')   
				return false;
        
				S.pop();  
			}
		}
		return S.empty();
	}
};
