class Solution{
    public:

    //Function to check if two arrays are equal or not.
    bool check(vector<ll> A, vector<ll> B, int n) {
       sort(A.begin(),A.end());
       sort(B.begin(),B.end());
       for(int i=0;i<n;i++)
       {
           if(A[i]!=B[i])
                return false;
                
       }
       return true;
    }
};
